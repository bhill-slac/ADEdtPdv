#
# EPICS database for
# Allied Vision Technoligies GoldEye-008CL
#
# Protocol root must match db filename root
# i.e. avtGoldEye008.proto

record( seq, "$(P)$(R)EdtReConfigSeq" )
{
	field( DLY1, "0.0" ) field( DOL1, "1" ) field( LNK1, "$(P)$(R)GoldEyeAccessPrivilege" )
	field( DLY2, "0.2" ) field( DOL2, "1" ) field( LNK2, "$(P)$(R)Manufacturer_RBV.PROC" )
	field( DLY3, "0.2" ) field( DOL3, "1" ) field( LNK3, "$(P)$(R)Model_RBV.PROC" )
	field( DLY4, "0.2" ) field( DOL4, "1" ) field( LNK4, "$(P)$(R)GoldEyeFirmwareVersion_RBV.PROC" )
	field( DLY5, "0.2" ) field( DOL5, "1" ) field( LNK5, "$(P)$(R)GoldEyeSerialNumber_RBV.PROC" )
	field( DLY6, "0.2" ) field( DOL6, "1" ) field( LNK6, "$(P)$(R)GoldEyeModel_RBV.PROC" )
	field( DLY7, "0.2" ) field( DOL7, "1" ) field( LNK7, "$(P)$(R)GoldEyeTempSensorBd_RBV.PROC" )
	field( DLY8, "0.2" ) field( DOL8, "1" ) field( LNK8, "$(P)$(R)GoldEyeTemperature_RBV.PROC" )
#   field( TPRO, 1 )
	field( FLNK, "$(P)$(R)EdtReConfigSeq2" )
}
record( seq, "$(P)$(R)EdtReConfigSeq2" )
{
	field( DLY1, "0.0" ) field( DOL1, "1" ) field( LNK1, "$(P)$(R)GoldEyeFStartFullTrMode_RBV.PROC" )
	field( DLY2, "0.0" ) field( DOL2, "1" ) field( LNK2, "$(P)$(R)GoldEyeAcqStartFullTrMode_RBV.PROC" )
	field( DLY3, "0.0" ) field( DOL3, "1" ) field( LNK3, "$(P)$(R)GoldEyeAcqStopFullTrMode_RBV.PROC" )
#	field( FLNK, "$(P)$(R)EdtReConfigSeq3" )
}

